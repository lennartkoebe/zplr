{
  "name": "zplr",
  "version": "0.1.1",
  "description": "TypeScript library for parsing and rendering ZPL label files",
  "main": "./dist/index.node.js",
  "module": "./dist/index.node.js",
  "types": "./dist/index.node.d.ts",
  "type": "module",
  "exports": {
    "./node": {
      "import": "./dist/index.node.js",
      "types": "./dist/index.node.d.ts"
    },
    "./web": {
      "import": "./dist/index.web.js",
      "types": "./dist/index.web.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "pnpm run dev:web",
    "dev:web": "vite",
    "dev:node": "tsx src/examples.ts --env NODE",
    "debug": "tsx --inspect-brk --watch src/main",
    "test": "vitest",
    "build": "tsup",
    "build:web": "vue-tsc --noEmit -p tsconfig.web.json && vite build",
    "preview:web": "vite preview",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json"
  },
  "keywords": [
    "zpl",
    "label",
    "barcode",
    "parser",
    "renderer",
    "canvas"
  ],
  "author": "Lennart Kurth",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@types/svgdom": "^0.1.2",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.0",
    "esbuild-plugin-path-alias": "^1.0.7",
    "pdf-visual-diff": "^0.14.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "tsup": "^8.5.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite": "^5.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8",
    "vue": "^3.4.0",
    "vue-tsc": "^3.1.1"
  },
  "dependencies": {
    "@svgdotjs/svg.js": "^3.2.4",
    "@types/qrcode": "^1.5.5",
    "canvas": "^2.11.2",
    "jsbarcode": "^3.11.6",
    "odiff-bin": "^3.2.0",
    "qrcode": "^1.5.4",
    "skia-canvas": "^2.0.0",
    "svgdom": "^0.1.19"
  }
}
